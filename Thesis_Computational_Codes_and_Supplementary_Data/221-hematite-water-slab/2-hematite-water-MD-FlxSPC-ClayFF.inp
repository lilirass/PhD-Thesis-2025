#
# MD of liquid water, 
# using FlxSPC and ClayFF
#

@SET SYSTEM run-H2O-MD-T300

&GLOBAL
  PROJECT ${SYSTEM}
  RUN_TYPE MD
  PRINT_LEVEL MEDIUM
  &PRINT
   PHYSCON
  &END PRINT
&END GLOBAL

&FORCE_EVAL
  METHOD FIST
  &MM
    &PRINT
      !&DIPOLE
       !PERIODIC TRUE 
       !&EACH
         !MD 1
       !&END
       !FILENAME =dipole.traj
      !&END DIPOLE
    &END
    &FORCEFIELD
      &BEND
        ATOMS HW OW HW   #FlxSPC
        K [kcalmol*rad^-2] 91.55177
        KIND harmonic
        THETA0 [deg] 109.47
      &END BEND
      &BOND
        ATOMS OW HW   #FlxSPC
        K [kcalmol*Angstrom^-2] 1106.001
        KIND harmonic
        R0 [Angstrom] 1.0
      &END BOND
      &BOND
        ATOMS H O_H  #ClayFF
        K [kcalmol*Angstrom^-2] 1106.001
        KIND harmonic
        R0 [Angstrom] 1.0
      &END BOND
      &CHARGE
        ATOM OW  #FlxSPC
        CHARGE  -0.820
      &END CHARGE
      &CHARGE
        ATOM HW  #FlxSPC
        CHARGE +0.410
      &END CHARGE
      &CHARGE
        ATOM Fe  #ClayFF
        CHARGE +1.575
      &END CHARGE
      &CHARGE
        ATOM O   #ClayFF
        CHARGE -1.050
      &END CHARGE
      &CHARGE   #ClayFF
        ATOM O_H    
        CHARGE -0.950
      &END CHARGE
      &CHARGE
        ATOM H  #ClayFF
        CHARGE +0.425
      &END CHARGE

      &NONBONDED
        &LENNARD-JONES
          ATOMS HW HW  #based on Flexible simple point-charge water model paper for SPC
          EPSILON [kcalmol] 0.0
          SIGMA [Angstrom] 1.0
          RCUT [Angstrom] 5.0
        &END LENNARD-JONES
	 &LENNARD-JONES  #all H-H epsolon are 0 
          ATOMS H H
          EPSILON [kcalmol] 0.0
          SIGMA [Angstrom] 1.0
          RCUT [Angstrom] 5.0
        &END LENNARD-JONES
        &LENNARD-JONES  #all H-H epsilon are 0
          ATOMS H HW
          EPSILON [kcalmol] 0.0
          SIGMA [Angstrom] 1.0
          RCUT [Angstrom] 5.0
        &END LENNARD-JONES
        &LENNARD-JONES
          ATOMS OW HW #based on Flexible simple point-charge water model paper for SPC
          EPSILON [kcalmol] 0.0
          SIGMA [Angstrom] 1.0
          RCUT [Angstrom] 5.0
        &END LENNARD-JONES
	 &LENNARD-JONES
          ATOMS O HW #based on Flexible simple point-charge water model paper for SPC
          EPSILON [kcalmol] 0.0
          SIGMA [Angstrom] 1.0
          RCUT [Angstrom] 5.0
        &END LENNARD-JONES
	&LENNARD-JONES
          ATOMS O_H HW #based on Flexible simple point-charge water model paper for SPC
          EPSILON [kcalmol] 0.0
          SIGMA [Angstrom] 1.0
          RCUT [Angstrom] 5.0
        &END LENNARD-JONES
	&LENNARD-JONES
          ATOMS OW H   #based on Flexible simple point-charge water model paper for SPC
          EPSILON [kcalmol] 0.0
          SIGMA [Angstrom] 1.0
          RCUT [Angstrom] 5.0
        &END LENNARD-JONES
	&LENNARD-JONES
          ATOMS O H     #based on Flexible simple point-charge water model paper for SPC
          EPSILON [kcalmol] 0.0
          SIGMA [Angstrom] 1.0
          RCUT [Angstrom] 5.0
        &END LENNARD-JONES
