          EPSILON [kcalmol] 0.0
          SIGMA [Angstrom] 1.0
          RCUT [Angstrom] 5.0
        &END LENNARD-JONES
	&LENNARD-JONES
          ATOMS H Fe #it is notdefined -> assigend 0
          EPSILON [kcalmol] 0.0
          SIGMA [Angstrom] 1.0
          RCUT [Angstrom] 5.0
        &END LENNARD-JONES


        &LENNARD-JONES
          ATOMS OW OW   #FlxSPC
          EPSILON [kcalmol] 0.1554
          SIGMA [Angstrom] 3.1655
          RCUT [Angstrom] 5.0
        &END LENNARD-JONES
	&LENNARD-JONES
          ATOMS OW O  #ClayFF
          EPSILON [kcalmol] 0.1554
          SIGMA [Angstrom] 3.1655
          RCUT [Angstrom] 5.0
        &END LENNARD-JONES
        &LENNARD-JONES
          ATOMS O O  #ClayFF
          EPSILON [kcalmol] 0.1554
          SIGMA [Angstrom] 3.1655
          RCUT [Angstrom] 5.0
        &END LENNARD-JONES
        &LENNARD-JONES
          ATOMS O O_H  #ClayFF
          EPSILON [kcalmol] 0.1554
          SIGMA [Angstrom] 3.1655
          RCUT [Angstrom] 5.0
        &END LENNARD-JONES
	&LENNARD-JONES
          ATOMS O_H O_H  #ClayFF
          EPSILON [kcalmol] 0.1554
          SIGMA [Angstrom] 3.1655
          RCUT [Angstrom] 5.0
        &END LENNARD-JONES
        &LENNARD-JONES
          ATOMS O_H OW  #ClayFF
          EPSILON [kcalmol] 0.1554
          SIGMA [Angstrom] 3.1655
          RCUT [Angstrom] 5.0
        &END LENNARD-JONES

	&LENNARD-JONES
          ATOMS Fe OW #ClayFF
          EPSILON [kcalmol] 3.4588
          SIGMA [Angstrom] 1.91545
          RCUT [Angstrom] 5.0
        &END LENNARD-JONES
        &LENNARD-JONES
          ATOMS Fe HW #not assigned -> 0 
          EPSILON [kcalmol] 0
          SIGMA [Angstrom] 1.0
          RCUT [Angstrom] 5.0
        &END LENNARD-JONES
     
        &LENNARD-JONES
          ATOMS Fe Fe #ClayFF
          EPSILON [kcalmol] 9.03E-06
          SIGMA [Angstrom] 4.072218
          RCUT [Angstrom] 5.0
        &END LENNARD-JONES
        &LENNARD-JONES
          ATOMS Fe O #ClayFF
          EPSILON [kcalmol] 0.001185
          SIGMA [Angstrom] 3.618831
          RCUT [Angstrom] 5.0
        &END LENNARD-JONES
        &LENNARD-JONES
          ATOMS Fe O_H #ClayFF
          EPSILON [kcalmol] 0.001185
          SIGMA [Angstrom] 3.618831
          RCUT [Angstrom] 5.0
        &END LENNARD-JONES
      &END NONBONDED
      &SPLINE
        EMAX_SPLINE [hartree] 5.0E-1
      &END SPLINE
    &END FORCEFIELD
    &POISSON
      PERIODIC XYZ
      &EWALD
        EWALD_TYPE spme
        ALPHA 0.3
        GMAX 10
        O_SPLINE 10
      &END EWALD
    &END POISSON
    &NEIGHBOR_LISTS
      GEO_CHECK T
    &END
  &END MM
